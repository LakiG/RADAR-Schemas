{
    "namespace": "org.radarcns.connector.upload.altoida",
    "name": "AltoidaContrastVisionTest",
    "type": "record",
    "doc": "Schema of events for the Altoida Tapping test.",
    "fields": [
      { "name": "time", "type": "double", "doc": "Timestamp in UTC (s) when the assessment is started by the subject." },
      { "name": "timeReceived", "type": "double", "doc": "Timestamp in unix time received from Altoida API."},
      { "name": "name", "type": "string", "doc": "Questionnaire names." },
      { "name": "contrastCircles", "type": {
        "type": "array",
        "items": {
          "name": "ContrastCircle",
          "type": "record",
          "doc": "Contrast circle screen element.",
           "fields": [
             { "name": "id", "type": ["null", "string"], "doc": "Circle id." },
             { "name": "radius", "type": "double", "doc": "Circle radius" },
             { "name": "xCenterOffset", "type": "double", "doc": "Circle location x center offset in cm." },
             { "name": "yCenterOffset", "type": "double", "doc": "Circle location y center offset in cm." },
             { "name": "colorAlphaValue", "type": ["null", "string"], "doc": "Circle alpha value in RGBA color space." },
             { "name": "colorRValue", "type": ["null", "string"], "doc": "Circle red value in RGBA color space." },
             { "name": "colorGValue", "type": ["null", "string"], "doc": "Circle green value in RGBA color space." },
             { "name": "colorBValue", "type": ["null", "string"], "doc": "Circle blue value in RGBA color space." }
           ]
         }}, "doc": "Contrast circles list."},
      { "name": "events", "type": {
        "type": "array",
        "items": {
          "name": "AssessmentEvent",
          "type": "record",
          "doc": "Assessment event.",
           "fields": [
             { "name": "eventType", "type": ["null", "string"], "doc": "Event type. Possible values: start_pressed, circle_pressed.", "default": null },
             { "name": "timestamp", "type": "double", "doc": "Timestamp in UTC (s) when the event occurred." },
             { "name": "location", "type": ["null", "string"], "doc": "Tapping location." }
           ]
         }}, "doc": "Assessment events list."}
    ]
  }
  